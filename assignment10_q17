package day25_assisgnment10;

public class assignment10_q17 {

	public static void main(String[] args) {

/*
Question-17

Write a program that will print out letters
in the alphabetic order accordingly to
the given range within 2 chars.

Sample Output:

     input: A
     input: Z
     output: ABCDEFGHIJKLMNOPQRSTUVWXYZ

     input: a
     input: f
     output: abcdef

     input: a
     input: d
     output: abcd

     input: B
     input: O
     output: BCDEFGHIJKLMNO 
 */
		
		print_Chars('A', 'T');
    }
	public static void print_Chars(char char1, char char2) {
		
        for (String i="char1"; char1 <= char2; char1++) {
            
        	System.out.print(char1 + " ");
         
        }
	}
	
}
